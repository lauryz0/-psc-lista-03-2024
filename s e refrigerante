        System.out.println("Digite o valor da compra: ");
        double valorCompra = 
        scanner.nextDouble();

        System.out.println(" Essa quantidade paga é insuficiente para relizar comprar.");
        } else {
        double troco = 
     valorPago - valorCompra;
        int[] {50, 20, 10, 5, 2, 1}; 
        int[] quantidadeNotas = new int [6];
           for (int i = 0; i < 6; i++)} 
           if (troco >= notas[i]) {
            quantidadeNotas[i] = (int) (troco / notas[i]);
            troco = troco % notas[i];
        }
    }

    System.out.println("O troco é:");
    for (int i = 0; i < 6; i++) {
        if (quantidadeNotas[i] != 0) {
